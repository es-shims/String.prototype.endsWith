		"./shim": "./shim.js",
		"./package.json": "./package.json"
	},
	"keywords": [
		"string",
		"endswith",
		"es6",
		"ecmascript",
		"polyfill"
	],
	"license": "MIT",
	"author": {
		"name": "Mathias Bynens",
